{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "817dca8b-572a-4554-aff3-fdd8392a2d13",
        "UseHttps": true,
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "Unattended": {
        "UpgradeUnattended": true
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto"
      },
      "Security": {
        "AllowConcurrentLogins": false
      }
    },
    "Slimsy": {
      "WidthStep": 80,
      "UseCropAsSrc": false,
      "DefaultQuality": 70,
      "Format": "",
      "BackgroundColor": "",
      "AppendSourceDimensions": true,
      "EncodeCommas": true,
      "AutoOrient": true
    }
  }
}
